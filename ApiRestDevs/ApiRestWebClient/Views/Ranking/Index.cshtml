@model IEnumerable<ApiRestWebClient.Models.ViewModels.RankingViewModel>

@{
    ViewData["Title"] = "Projetos";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-sm-12">
        <table id="token" name="token" class="table table-responsive table-hover table-striped">

            <thead>
                <tr>
                    <th>Posi&Ccedil;&atilde;o no Ranking</th>
                    <th>Nome do Desenvolvedor</th>
                    <th>M&eacute;dia de Horas</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>@item.PosicaoDoRanking</td>
                    <td>@item.NomeDoDesenvolvedor</td>
                    <td>@item.MediaDeHoras</td>
                </tr>
                }

            </tbody>

        </table>
    </div>
</div>

