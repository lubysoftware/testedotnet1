@model ApiRestWebClient.Models.ViewModels.TokenViewModel

@{
    ViewData["Title"] = "TOKEN";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-sm-12">
        <table id="token" name="token" class="table table-responsive table-hover table-striped">

            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.User)</th>
                    <th>@Html.DisplayNameFor(model => model.Token)</th>
                </tr>
            </thead>

            <tbody>
                <tr>
                    <td>@Model.User</td>
                    <td><label>@Model.Token</label></td>
                </tr>
            </tbody>

        </table>
    </div>
</div>

