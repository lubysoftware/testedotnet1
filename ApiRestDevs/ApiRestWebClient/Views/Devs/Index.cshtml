@model IEnumerable<ApiRestWebClient.Models.ViewModels.DevsViewModel>

@{
    ViewData["Title"] = "DEVS";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-sm-12">
        <table id="token" name="token" class="table table-responsive table-hover table-striped">

            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(model => model.Id)</th>
                    <th>@Html.DisplayNameFor(model => model.Nome)</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Nome</td>
                    </tr>
                }

            </tbody>

        </table>
    </div>
</div>
